<thead>
  <tr>
    <th>Log ID</th>
    <th>Date</th>
    <th>Workout</th>
    <th>Athlete</th>
  </tr>
</thead>
<tbody>
  {{#each logs as |log|}}
  <tr data-id="{{log.id}}">
    <th scope="row">{{log.id}}</th>
    <td>{{log.date_completed}}</td>
    <td>{{log.workout.name}}</td>
    <td>{{log.athlete.given_name}} {{log.athlete.surname}}</td>
  </tr>
  {{/each}}
</tbody>
